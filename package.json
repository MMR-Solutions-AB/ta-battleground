{
  "name": "ta-battleground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn db:seed:prod && next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "gen": "tsx ./src/scripts/generateNewProblem.ts",
    "db:push": "npx prisma db push",
    "db:seed": "NODE_ENV=development tsx ./prisma/seed-dev.ts",
    "db:seed:prod": "NODE_ENV=production prisma db seed",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write ./src",
    "test": "vitest",
    "count": "git ls-files --exclude-standard -- ':!:**/*.[pjs][npv]g' ':!:**/*.jpeg' ':!:**/*.webp' ':!:yarn.lock' ':!:package-lock.json' | xargs wc -l",
    "count-all": "git ls-files | xargs wc -l",
    "count-files": "git ls-files | wc -l"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.2",
    "@monaco-editor/react": "^4.4.6",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.5.0",
    "@tanstack/react-query": "^4.16.0",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "@uiw/codemirror-themes-all": "^4.19.5",
    "@uiw/react-codemirror": "^4.19.5",
    "classnames": "^2.3.2",
    "codemirror": "^6.0.1",
    "js-beautify": "^1.14.7",
    "lodash": "^4.17.21",
    "marked": "^4.2.5",
    "next": "13.0.2",
    "next-auth": "^4.18.3",
    "node-fetch": "^3.3.0",
    "party-js": "^2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "^2.0.10",
    "superjson": "1.9.1",
    "terser": "^5.16.1",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/react": "^13.4.0",
    "@types/inquirer": "^9.0.3",
    "@types/js-beautify": "^1.13.3",
    "@types/lodash": "^4.14.191",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.2.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.2",
    "inquirer": "^9.1.4",
    "jsdom": "^20.0.3",
    "monaco-editor": "^0.34.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.5.0",
    "tailwindcss": "^3.2.0",
    "tsx": "^3.12.1",
    "typescript": "^4.8.4",
    "vitest": "^0.26.3"
  },
  "ct3aMetadata": {
    "initVersion": "6.11.4"
  }
}
